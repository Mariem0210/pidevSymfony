{% extends 'base.html.twig' %}

{% block title %}Equipe index{% endblock %}

{% block body %}
    <h1>Equipe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Ideq</th>
                <th>Nom_equipe</th>
                <th>Date_creation</th>
                <th>Idu</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for equipe in equipes %}
            <tr>
                <td>{{ equipe.ideq }}</td>
                <td>{{ equipe.nomEquipe }}</td>
                <td>{{ equipe.dateCreation ? equipe.dateCreation|date('Y-m-d') : '' }}</td>
                <td>{{ equipe.idu }}</td>
                <td>
                    <a href="{{ path('app_equipe_show', {'ideq': equipe.ideq}) }}">show</a>
                    <a href="{{ path('app_equipe_edit', {'ideq': equipe.ideq}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_equipe_new') }}">Create new</a>
{% endblock %}
